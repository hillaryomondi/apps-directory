{
    "name": "strathmore/translatable",
    "description": "Make your Eloquent models and other content translated into multiple languages",
    "keywords": [
        "translations",
        "eloquent",
        "language",
        "locale",
        "translate",
        "localization",
        "admin",
        "cms",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Samson Maosa",
            "email": "smaosa@strathmore.edu",
            "homepage": "https://www.savannabits.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "laravel/framework": "6.*|7.*",
        "spatie/laravel-translatable": "^4.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "~4.0"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Strathmore\\Translatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strathmore\\Translatable\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Strathmore\\Translatable\\TranslatableServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
